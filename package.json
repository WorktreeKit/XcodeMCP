{
  "name": "xcodemcp",
  "version": "1.3.2",
  "description": "Model Context Protocol server for Xcode build automation and log parsing",
  "main": "index.js",
  "type": "module",
  "bin": {
    "xcodemcp": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18"
  },
  "os": [
    "darwin"
  ],
  "keywords": [
    "mcp",
    "xcode",
    "build",
    "automation",
    "ios",
    "macos",
    "swift",
    "xclogparser",
    "model-context-protocol"
  ],
  "author": "Felix Lapalme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lapfelix/XcodeMCP.git"
  },
  "homepage": "https://github.com/lapfelix/XcodeMCP#readme",
  "bugs": {
    "url": "https://github.com/lapfelix/XcodeMCP/issues"
  }
}
